@model List<QuantitaAggiunta>

    @foreach(QuantitaAggiunta categoria in Model)
                        {
                           <h1>@categoria.ProdottiVenduti</h1> 
                           <marquee behavior="scroll" scrollamount="3" direction="left" width="350">
                                <img class="oggetto" src="@categoria.Immagine" alt="">
                                <h5 class="text">@categoria.Nome</h5> 
                         </marquee>
                        }
